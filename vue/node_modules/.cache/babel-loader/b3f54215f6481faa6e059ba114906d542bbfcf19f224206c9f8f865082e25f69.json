tion=\"direction\"\r\n  :before-close=\"handleClose\">\r\n\r\n    <el-container>\r\n    <el-main>\r\n      <div>\r\n    <el-date-picker\r\n      v-model=\"value1\"\r\n      type=\"date\"\r\n      placeholder=\"选择日期\"\r\n      value-format=\"yyyy-MM-dd\">\r\n    </el-date-picker>\r\n    <el-table\r\n    ref=\"singleTable\"\r\n    :data=\"tableData_2\"\r\n    highlight-current-row\r\n    @current-change=\"handleCurrentChange\"\r\n    style=\"width: 100%\">\r\n    <el-table-column\r\n      fixed\r\n      prop=\"cid\"\r\n      label=\"摄像头编号\"\r\n      width=\"150\">\r\n    </el-table-column>\r\n    <el-table-column\r\n      fixed\r\n      prop=\"content\"\r\n      label=\"描述\"\r\n      width=\"750\">\r\n    </el-table-column>\r\n    <el-table-column\r\n      fixed\r\n      prop=\"owner\"\r\n      label=\"是否为管理员\"\r\n      width=\"140\">\r\n    </el-table-column>\r\n  </el-table>\r\n  </div>\r\n  <el-footer>\r\n    <div style=\"text-align: center\">\r\n      <el-button type=\"primary\" @click=\"chdate\" >提交</el-button>\r\n    </div>\r\n    \r\n  </el-footer>\r\n  \r\n</el-main>\r\n</el-container>\r\n</el-drawer>\r\n        <video \r\n        :src=this.videoUrl\r\n        width=\"870\"\r\n        height=\"490\"\r\n        autoplay\r\n        controls\r\n        >\r\n        </video>\r\n    \r\n    <div>\r\n        <el-button @click=\"drawer = true\" type=\"primary\" style=\"margin-left: 16px;\">\r\n            点我打开\r\n        </el-button>\r\n        <div class=\"block\">\r\n    <el-slider\r\n      v-model=\"value\"\r\n      :marks=\"marks\">\r\n    </el-slider>\r\n  </div>\r\n    </div>\r\n</div>\r\n    \r\n</template>\r\n\r\n<script>\r\n    import axios from 'axios'\r\n    import global from './GlobalPage.vue'\r\n  const restweburl = global.ip;\r\n  export default {\r\n    data(){\r\n        return {\r\n            // videoUrl: 'https://dbd-video-0.oss-cn-beijing.aliyuncs.com/A.mp4',\r\n            drawer:true,\r\n            value:0,\r\n            marks: {\r\n          0: '0°C',\r\n          8: '8°C',\r\n          37: '37°C',\r\n          50: {\r\n            style: {\r\n              color: '#1989FA'\r\n            },\r\n            label: this.$createElement('strong', '50%')\r\n          }\r\n        }\r\n        }\r\n    },\r\n    created() {\r\n        axios.get(restweburl + \"getPlayback\", {params:{'pID':this.$route.params.pid}})\r\n        .then((res) => {\r\n            this.videoUrl = res.data.data;\r\n        })\r\n        .catch(function (error) {\r\n          console.log(error);\r\n        });\r\n  },\r\n  methods: {\r\n      goBack() {\r\n        this.$router.go(-1)\r\n      },\r\n      chdate(){\r\n        var date = this.value1.split('-')\r\n        this.drawer=false;\r\n\r\n        this.date=date;\r\n        this.cid=this.currentRow.cid;\r\n\r\n        this.year=date[0]\r\n        this.month=date[1]\r\n        this.day = date[2]\r\n\r\n        axios.get(restweburl + \"playbacks\", {params:{'cID':this.currentRow.cid,'year':date[0],'month':date[1],'day':date[2]}})\r\n        .then((res) => {\r\n          this.tableData = res.data.data;\r\n          this.loading=false;\r\n        })\r\n        .catch(function (error) {\r\n          console.log(error);\r\n        });\r\n      }\r\n    }\r\n}\r\n</script>"],"mappings":"AA8EA,OAAAA,KAAA;AACA,OAAAC,MAAA;AACA,MAAAC,UAAA,GAAAD,MAAA,CAAAE,EAAA;AACA;EACAC,KAAA;IACA;MACA;MACAC,MAAA;MACAC,KAAA;MACAC,KAAA;QACA;QACA;QACA;QACA;UACAC,KAAA;YACAC,KAAA;UACA;UACAC,KAAA,OAAAC,cAAA;QACA;MACA;IACA;EACA;EACAC,QAAA;IACAZ,KAAA,CAAAa,GAAA,CAAAX,UAAA;MAAAY,MAAA;QAAA,YAAAC,MAAA,CAAAD,MAAA,CAAAE;MAAA;IAAA,GACAC,IAAA,CAAAC,GAAA;MACA,KAAAC,QAAA,GAAAD,GAAA,CAAAd,IAAA,CAAAA,IAAA;IACA,GACAgB,KAAA,WAAAC,KAAA;MACAC,OAAA,CAAAC,GAAA,CAAAF,KAAA;IACA;EACA;EACAG,OAAA;IACAC,OAAA;MACA,KAAAC,OAAA,CAAAC,EAAA;IACA;IACAC,OAAA;MACA,IAAAC,IAAA,QAAAC,MAAA,CAAAC,KAAA;MACA,KAAA1B,MAAA;MAEA,KAAAwB,IAAA,GAAAA,IAAA;MACA,KAAAG,GAAA,QAAAC,UAAA,CAAAD,GAAA;MAEA,KAAAE,IAAA,GAAAL,IAAA;MACA,KAAAM,KAAA,GAAAN,IAAA;MACA,KAAAO,GAA